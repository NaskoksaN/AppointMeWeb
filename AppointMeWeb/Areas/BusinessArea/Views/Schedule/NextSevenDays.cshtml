@model IEnumerable<BusinessAppointmentViewModel>
@{
    ViewData["Title"] = $"Upcoming Appointments for the Next 7 Days: {DateTime.Now.AddDays(1).ToString(NameDayOfWeek)}, {DateTime.Now.AddDays(1).ToString(DateMonthYearFormat)} - {DateTime.Now.AddDays(7).ToString(NameDayOfWeek)}, {DateTime.Now.AddDays(7).ToString(DateMonthYearFormat)}";
}
<h4 class="text-center">@ViewData["Title"]</h4>

<div class="container mt-4">
    <div class="row">
        <partial name="_AppointmentCard" model="Model" />
    </div>
</div>

<script>
    function cancelAppointment(appointmentId) {
        alert('Cancel appointment with ID: ' + appointmentId);

    }
</script>
