@model IEnumerable<BusinessAppointmentViewModel>
@{
    ViewBag.Title = "Appointments from " + ViewBag.StartDate.ToString("d") + " to " + ViewBag.EndDate.ToString("d");
}

<h2>@ViewBag.Title</h2>

<div class="container mt-4">
    <div class="row">
        <partial name="_AppointmentCard" model="Model" />
    </div>
</div>

<script>
    function cancelAppointment(appointmentId) {
        alert('Cancel appointment with ID: ' + appointmentId);

    }
</script>
