@model IEnumerable<BusinessAppointmentViewModel>
@{
    ViewData["Title"] = $"Appointments for today: {DateTime.Now.ToString(NameDayOfWeek)}, {DateTime.Now.ToString(DateMonthYearFormat)}";
}
<h4 class="text-center">@ViewData["Title"]</h4>

<div class="container mt-4">
    <div class="row">
        <partial name="_AppointmentCard" model="Model" />
    </div>
</div>

<script>
    function cancelAppointment(appointmentId) {
        alert('Cancel appointment with ID: ' + appointmentId);
        
    }
</script>